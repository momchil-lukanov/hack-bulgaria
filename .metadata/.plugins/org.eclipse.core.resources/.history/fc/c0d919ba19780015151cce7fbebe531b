package application;

public class WordGame {
	
	public static String stringReverse(String a){
		String result = "";
		for (int i = 0; i < a.length(); i++) {
			result = a.charAt(i) + result;
		}
		return result;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
