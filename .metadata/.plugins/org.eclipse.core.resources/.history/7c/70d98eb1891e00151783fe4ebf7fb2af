package com.hackbulgaria.programming51.week4;

public class MusicOrganiser {

	private Vector<Song> songs;

	public MusicOrganiser() {
		songs = new Vector<Song>();
	}

	public void addSong(Song x) {
		songs.add(x);
	}
	
	public String toString(){
		String result = "";
		for (song: songs) {
			result += song + ","; 
		}
		return result;
	}

}
