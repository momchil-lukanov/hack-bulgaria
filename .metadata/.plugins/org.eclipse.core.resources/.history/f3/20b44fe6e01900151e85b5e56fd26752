package com.hackbulgaria.programming51.week2;

public class AutoShop {
	
	public Auto[] autos = new Auto[1000];
	int counterAuto = 0;
	
	public void addCar(Auto car){
		autos[0] = car;
		counterAuto++;
	}
	
	public Auto getTopSpeed(){
		int maxSpeed = 0;
		for (int i = 0; i < autos.length; i++) {
			if (maxSpeed < autos[i].maxSpeed){
				maxSpeed = autos[i].maxSpeed;
			}
		}
		return maxSpeed;
	}
	
	
	
}
