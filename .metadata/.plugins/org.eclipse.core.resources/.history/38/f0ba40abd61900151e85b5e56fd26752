package com.hackbulgaria.programming51.week2;

public class CoffeeMachineMain {

	public static void main(String[] args) {
		CoffeeMachine cm = new CoffeeMachine();
		Coffee java = new Coffee();
		Coffee latte = new Coffee();
		Coffee lat = new Coffee();

		java.brand = "Java";
		java.price = 1.00;

		latte.brand = "Latte";
		latte.price = 1.50;
		
		lat.brand = "Lat";
		lat.price = 1.50;

		System.out.println(cm.addCoffee(java));
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(latte);
		cm.addCoffee(lat);
	}

}
