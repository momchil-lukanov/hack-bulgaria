			for (int j = 0; j < sth.length(); j++) {
				Character sth2= sth.charAt(j);
				if (sth2.equals(' ') && counterOfSpaces == 0) {
					functionNames.addElement(sth.substring(0, j));
					counterOfSpaces++;
				}
				if (sth2.equals('=')) {
					for (int k = 0; k < sth.substring(j, sth.length()); k++) {
						
					}
				}

			}
		}
		return x;
		
	}

