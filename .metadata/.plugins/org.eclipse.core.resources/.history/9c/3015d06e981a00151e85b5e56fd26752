package com.hackbulgaria.programming51.week3;

public class IntegerVectorMain {

	public static void main(String[] args) {
		   IntegerVector v = new IntegerVector();
		    for (int i = 0; i < 30; i++) {
			v.add(i);
		    }
		    System.out.println(v);
		    
		    IntegerVector v = new IntegerVector();
		    for (int i = 0; i < 20; i++) {
		        v.add(i);
		    }
		    v.add(50);
		    System.out.println(v.size());
	}

}
