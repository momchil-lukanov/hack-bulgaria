package week1;

import java.util.Scanner;

public class tasks {
	
	public static boolean isOdd(int n) {
		if (n % 2 != 0) {
			return true;
		} else {
			return false;
		}
	}

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		

	}

}
