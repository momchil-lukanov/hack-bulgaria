public class LabirinthMain {

	public static void main(String[] args) {

		char[][] symbols = { { '#', '#', '#', '#', '#', '#', '#', '#' },
				{ '#', 'X', '.', '#', '.', '.', '.', '#' },
				{ '#', '.', '.', '.', '.', '#', '.', '#' },
				{ '#', '.', '.', '#', '.', '.', '.', '#' },
				{ '#', '#', '.', '.', '.', '.', '#', '#' },
				{ '#', '.', '.', '.', '#', '.', '.', '#' },
				{ '#', '#', '#', '#', '#', '#', '!', '#' } };

		Labirinth sth = new Labirinth(symbols);
		
		System.out.println(sth.toString(sth));

	}
}
